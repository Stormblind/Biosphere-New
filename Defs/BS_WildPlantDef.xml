<Defs>

  <ThingDef ParentName="VG_Plant_Barley" Abstract="True">
    <defName>BS_Wild_Barley</defName>
    <label>barley plant</label>
    <description>A cereal grain. Hearty plant that is imune to blight. Used in cooking and for making ale.</description>
    <plant>
      <neverBlightable>true</neverBlightable>
      <harvestedThingDef>RawBarley</harvestedThingDef>
      <harvestYield>8</harvestYield>
      <topWindExposure>0.35</topWindExposure>
      <growDays>7</growDays>
      <fertilitySensitivity>0.6</fertilitySensitivity>
      <immatureGraphicPath>Things/Immature/BarleyPlant</immatureGraphicPath>
      <wildClusterRadius>8</wildClusterRadius>
      <wildClusterWeight>24</wildClusterWeight>
      <wildOrder>2</wildOrder>
      <purpose>Food</purpose>
    </plant>
  </ThingDef>
          
