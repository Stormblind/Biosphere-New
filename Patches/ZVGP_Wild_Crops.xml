            </li>
            
            <!-- Wild_squash -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_squash"]</xpath>
               <attribute>Abstract</attribute>
            </li>
            
            <!-- Wild_Tomato -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_Tomato"]</xpath>
               <attribute>Abstract</attribute>
            </li>
            
            <!-- Wild_Carrot -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_Carrot"]</xpath>
