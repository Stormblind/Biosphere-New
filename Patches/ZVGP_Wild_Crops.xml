            <!-- Wild_MangoTree -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_MangoTree"]</xpath>
               <attribute>Abstract</attribute>
            </li>


            <!-- VGP_WildBiomes_Removals -->
            <li Class="PatchOperationRemove">
              <xpath>Defs/ThingDef[defName="Plantmushroom" or defName="PlantTreeRedMaple" or defName="PlantTreeAcacia" or defName="PlantTreeCamellia" or defName="PlantSunflower" or defName="PlantPinkRimstalk" or defName="PlantNymphHair" or defName="PlantAnthurium" or defName="PlantTreeIronwood" or defName="PlantPlumeria" or defName="WildPlantpineapple" or defName="WildPlantCherrytree" or defName="WildCoconuttree" or defName="WildFigTree" or defName="WildDateTree" or defName="WildPeachTree" or defName="WildPlantcloudberry" or defName="WildPlantgooseberry" or defName="WildAppletree" or defName="WildBananaTree" or defName="WildorangeTree"]/plant/wildBiomes</xpath>
            </li>

          </operations>
      </match>
  </Operation>
</Patch>
