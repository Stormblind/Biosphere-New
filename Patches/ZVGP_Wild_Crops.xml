            </li>
              <!-- Wild_cocoa -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_cocoa"]</xpath>
               <attribute>Abstract</attribute>
            </li>
            
            <!-- Wild_grape -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_grape"]</xpath>
               <attribute>Abstract</attribute>
            </li>
            
            <!-- Wild_blueberry -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_blueberry"]</xpath>
