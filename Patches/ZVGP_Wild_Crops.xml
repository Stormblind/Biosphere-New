<?xml version="1.0" encoding="UTF-8"?>
<Patch>
  <!--VGP wild Crops -->
  <Operation Class="PatchOperationFindMod">
    <mods>
        <li>VGP Vegetable Garden</li>
    </mods>
        <match Class="PatchOperationSequence">
          <success>Always</success>
          <operations>
                    <!-- Wild_Snowbeet -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_Snowbeet"]</xpath>
               <attribute>Abstract</attribute>
            </li>
                    <!-- Wild_Barley -->
             <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_Barley"]</xpath>
                <attribute>Abstract</attribute>
            </li>
                    <!-- Wild_redlentil -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_redlentil"]</xpath>
               <attribute>Abstract</attribute>
            </li>
              <!-- Wild_bean -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_bean"]</xpath>
               <attribute>Abstract</attribute>
            </li>
            <!-- Wild_Olivetree -->
            <li Class="PatchOperationAttributeRemove">
              <xpath>Defs/ThingDef[defName="BS_Wild_Olivetree"]</xpath>
               <attribute>Abstract</attribute>
            </li>
              <!-- Wild_Tea -->
